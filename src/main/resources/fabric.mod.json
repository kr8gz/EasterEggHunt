{
  "schemaVersion": 1,
  "id": "egghunt",
  "version": "${version}",
  "name": "Egg Hunt",
  "description": "A simple server-side easter egg hunt mod for Fabric.",
  "authors": [
    "kr8gz"
  ],
  "contact": {
    "issues": "https://github.com/kr8gz/EggHunt/issues",
    "sources": "https://github.com/kr8gz/EggHunt"
  },
  "license": "MIT",
  "icon": "assets/egghunt/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "io.github.kr8gz.egghunt.EggHunt"
      }
    ]
  },
  "mixins": [
    "egghunt.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-language-kotlin": ">=1.10.18+kotlin.1.9.22",
    "sqlite-jdbc": ">=3.36.0.3"
  }
}
